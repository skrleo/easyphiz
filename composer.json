{
    "name": "skrleo/easyphiz",
    "description": "一个简单的PhizPay SDK扩展包.",
    "keywords": [
        "phizpay",
        "phizpay-sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "rio",
            "email": "skrleo@163.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-fileinfo": "*",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "PhizPay\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "PhizPay\\Tests\\": "tests/"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1"
    }
}
